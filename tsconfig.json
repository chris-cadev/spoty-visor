{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@layouts/*": ["layouts/*"],
      "@modules/*": ["modules/*"],
      "@assets/*": ["assets/*"],
      "@api/*": ["pages/api/*"]
    }
  }
}
